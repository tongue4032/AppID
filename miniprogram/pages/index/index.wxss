  height: 100%;
}
/**滚动图片**/
.pic {
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.pic swiper{
  margin-top: -45px;
  width: 100%;
  height: 380rpx;
}
.pic swiper image{
  width: 100%;
  height: 100%;
  border-radius: 15px;
}
